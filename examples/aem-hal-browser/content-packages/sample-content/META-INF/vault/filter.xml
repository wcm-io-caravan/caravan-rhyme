<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/content/dam/aem-hal-browser">
        <!-- Exclude folder thumbnails - they will be generated automatically -->
        <exclude pattern="^.*/jcr:content/folderThumbnail(/.*)?$"/>
    </filter>
    <filter root="/content/aem-hal-browser"/>
    <filter root="/conf/aem-hal-browser">
        <include pattern="/conf/aem-hal-browser/.*"/>
        <exclude pattern="/conf/aem-hal-browser/rep:policy(/.*)?"/>
        <exclude pattern="/conf/aem-hal-browser/settings/wcm(/.*)?"/>
    </filter>
</workspaceFilter>
